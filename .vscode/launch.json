{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Backend",
            "type": "node",
            "request": "launch",
            "runtimeArgs": [
                "run-script",
                "start:dev"
            ],
            "runtimeExecutable": "npm",
            "console": "integratedTerminal",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "cwd": "${workspaceFolder}/packages/backend"
        }
    ]
}